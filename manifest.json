{

  "manifest_version": 2,
  "name": "Cat Simulator",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icons/TreeHacks-white-19.png",
    "default_title": "Cal to Butt",
    "default_popup": "src/popup.html"
  },


  "permissions": [
    "storage"
],

"description": "Chrome Extension Hackpack @ TreeHacks 2019",
"icons": {
  "16": "icons/TreeHacks-white-16.png",
  "32": "icons/TreeHacks-white-32.png",
  "48": "icons/TreeHacks-white-48.png",
  "64": "icons/TreeHacks-white-64.png",
  "96": "icons/TreeHacks-white-96.png",
  "128": "icons/TreeHacks-white-128.png"
},

"background": {
  "scripts": ["src/background.js"]
},

  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["src/content.js"],
    "css": ["src/content.css"],
    "web_accessible_resources": ["src/assets/cat_tiles.png"]
  }
]
}
